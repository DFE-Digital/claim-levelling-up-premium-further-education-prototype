      value: {
        text: claim.claimantName
      }
    },
    {
      key: {
        text: "Date of birth"
      },
      value: {
        html: claim.dateOfBirth
      }
    },
    {
      key: {
        text: "TRN"
      },
      value: {
        html: claim.trn
      }
